{
    "id": "org.ekstep.qe.questionbank",
    "ver": "1.0",
    "shortId": "questionbank",
    "author": "Swati",
    "type": "plugin",
    "title": "questionbank",
    "description": "This plugin is for question bank",
    "editor": {
        "main": "editor/plugin.js",
        "dependencies": [
            { "type": "js", "src": "editor/init.js" },
            { "type": "html", "src": "editor/popup.js" }
        ],
        "menu": [{
            "id": "questionbank",
            "category": "main",
            "type": "image",
            "toolTip": "Add Questions",
            "title": "create Questions",
            "iconImage": "assets/createquestion.png",
            "onclick": {
                 "id": "org.ekstep.qe.questionbank:showpopup",
                "data": {
                    "type": "roundrect",
                    "x": 5,
                    "y": 5,
                    "fill": "#0000",
                    "w": 90,
                    "h": 90,
                    "radius": 10,
                    "stroke": "rgba(255, 255, 255, 255)",
                    "strokeWidth": 1,
                    "opacity": 0.01
                }
            }
        }],
        "help": {
            "src": "editor/help.md",
            "dataType": "text"
        }
    },
    "renderer": {
        "main": "renderer/plugin.js"
    }
}
