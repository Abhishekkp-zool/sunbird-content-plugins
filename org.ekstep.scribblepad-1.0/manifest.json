{
    "id": "org.ekstep.scribblepad",
    "ver": "1.0",
    "shortId": "scribble",
    "author": "Santhosh Vasabhaktula",
    "title": "Scribblepad Plugin",
    "description": "",
    "publishedDate": "",
    "editor": {
        "main": "editor/plugin.js",
        "dependencies": [
            { "type": "plugin", "plugin": "org.ekstep.colorpicker", "ver": "1.0" }
        ],
        "menu": [{
            "id": "scribblepad",
            "category": "main",
            "type": "image",
            "toolTip": "Add scribblepad",
            "title": "scribblepad",
            "iconImage": "assets/scribblepad.png",
            "onclick": {
                "id": "org.ekstep.scribblepad:create",
                "data": {
                    "type" : "roundrect",
                    "y": 20,
                    "x": 25,
                    "fill": "#3399FF",
                    "w": 27,
                    "h": 60,
                    "stroke": 1,
                    "strokeWidth": 2,
                    "strokeDashArray": [5, 5]
                }
            }
        }],
        "config": [{
            "id": "colorpicker",
            "type": "colorpicker",
            "toolTip": "Change Color",
            "title": "Color",
            "isDropdown": false,
            "iconClass": "jscolor",
            "iconStyle": "width:20px;height:20px;",
            "onclick": {
                "id": "colorpicker:show",
                "data": {}
            }
        }]
    },
    "initdata": {
        "props": {
            "y": 200,
            "x": 200
        }
    }
}
